    <script type="text/javascript" src="~/Content/User/assets/plugins/slider.revolution/js/jquery.themepunch.revolution.min.js"></script>
    <script type="text/javascript" src="~/Content/User/assets/js/view/demo.revolution_slider.js"></script>
